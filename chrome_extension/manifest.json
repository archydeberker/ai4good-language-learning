{
  "name": "Bienvenue",
  "version": "0.1",

  "manifest_version": 2,
  
  "browser_action": {
    "default_icon": "bienvenue.png"
  },

  "content_scripts": [{
    "js": ["article.js","inject.js"],
    "matches": ["<all_urls>"],
    "permissions": [
      "activeTab",
      "declarativeContent",
      "storage"
    ]
  }]
}